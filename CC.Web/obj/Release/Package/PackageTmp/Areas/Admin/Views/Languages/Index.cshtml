@model IEnumerable<CC.Data.AgencyGroup>
@{
	ViewBag.Title = "Languages";
}

<h2>@ViewBag.Title</h2>
<table id="data"></table>
<script>
    $(function () {
        var oTable = $('table#data').dataTable({
            "aaSorting": [[0, "asc"]],
            "bServerSide": true,
            "sAjaxSource": '@Url.Action("IndexData")',
            "aoColumns": [
				 { "mData": "Id", "sTitle": "Id" },
				 { "mData": "Name", "sTitle": "Name" },
            ]
        });
    });
</script>

